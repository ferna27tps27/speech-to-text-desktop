# Copy to .env and fill in your values. Never commit .env.
GEMINI_API_KEY=your_gemini_api_key_here

# Models
TTS_MODEL=gemini-2.5-flash-preview-tts
STT_MODEL=gemini-2.5-flash

# Live API for real-time dictation (much faster, lower latency)
USE_LIVE_API=true
LIVE_STT_MODEL=gemini-2.5-flash-native-audio-preview-12-2025
# Silence duration before ending speech (ms). Lower = faster but may cut off pauses
LIVE_SILENCE_DURATION_MS=800

# ElevenLabs TTS (RECOMMENDED - much faster than Gemini TTS)
# Free tier: 10,000 characters/month - perfect for personal use
# Latency: ~240ms vs Gemini's 20-60 seconds!
# Get your free API key at: https://elevenlabs.io/
USE_ELEVENLABS_TTS=true
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
# Fastest model with lowest latency
ELEVENLABS_MODEL=eleven_flash_v2_5
# Voice ID - default is "George". Find more voices at elevenlabs.io/voices
ELEVENLABS_VOICE_ID=JBFqnCBsd6RMkjVDRZzb

VOICE_NAME=Kore

# TTS style direction for controllable speech (leave empty to disable)
# Examples: "naturally", "cheerfully", "in a calm tone", "with enthusiasm",
#           "slowly and clearly", "in a professional tone", "warmly",
#           "in a friendly conversational manner", "with a slight British accent"
TTS_STYLE=naturally and clearly

WARMUP_TTS=true
